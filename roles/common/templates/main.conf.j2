{# This config is common to all Junos device types. For it to be generated the 'common' role must be included in the platform specific play file. #}
system {
    time-zone Europe/Berlin;
    no-redirects;
    authentication-order [ tacplus password ];
    root-authentication {
        encrypted-password "$1$rZSaOfdu$WkjrZFtNzv4rqemuUgfXL0"; ## SECRET-DATA
    }
    name-server {
        10.29.224.25;
        10.29.224.53;
        10.29.255.53;
    }
    tacplus-server {
        10.29.224.254 {
            port 49;
            secret "$9$BFVEhrKMX-bsWLxdbwJZ69A01heK8X-w"; ## SECRET-DATA
            timeout 10;
            single-connection;
        }
    }
    services {
        ssh {
            root-login deny;
            protocol-version v2;
            rate-limit 10;
        }
        netconf {
            ssh;
        }
    }
    login {
        class rancid {
            permissions [ access admin firewall flow-tap interface network routing secret security snmp system trace view ];
        }
        user exampleorg {
            uid 2000;
            class super-user;
            authentication {
                encrypted-password "$1$Cfleyry8$.344Wb06pWKZpjIbfH53P1"; ## SECRET-DATA
            }
        }
        user rancid {
            uid 2001;
            class rancid;
            authentication {
                ssh-rsa "ssh-rsa "; ## SECRET-DATA
            }
        }
        user remote {
            uid 2002;
            class read-only;
        }
        user sdavid {
            full-name "Steffen David";
            uid 2004;
            class super-user;
            authentication {
                encrypted-password "$1$w5Aho4H3$zEVvMRiCwiQKqzr3A.VGL1"; ## SECRET-DATA
            }
        }
    }
    syslog {
        user * {
            any emergency;
        }
        file messages {
            any notice;
            authorization info;
        }
        file interactive-commands {
            interactive-commands any;
        }
    }
    ntp {
        server 10.29.224.53;
        server 10.29.255.53;
    }
}
snmp {
    contact "noc@exampleorg.de";
    community exampleorg1ntern {
        authorization read-only;
    }
    trap-group default {
        categories {
            authentication;
            chassis;
            link;
            remote-operations;
            routing;
            startup;
            rmon-alarm;
            vrrp-events;
            configuration;
        }
        targets {
            10.29.224.25;
            10.29.224.23;
        }
    }
}
